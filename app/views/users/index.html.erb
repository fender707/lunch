
<h1>Listing users</h1>

<table>
  <tr>
      <th>Name<th>
      <th>Email</th>
      <th>Created_at</th>
      <th class="offset">Destroy</th>

  </tr>

<% @users.each do |user| %>
  <tr class="offset">
    <td><%= user.username %></td>
    <td></td>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
  </tr>
<% end %>
</table>
